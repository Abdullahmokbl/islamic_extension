{
  "name": "islamic",
  "displayName": "Islamic",
  "description": "",
  "version": "0.0.1",
  "icon": "./index.jpeg",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HussamAdil/Ayat.git"
  },
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "islamic.getAya",
        "category": "Islamic",
        "title": "Get Aya",
        "icon": "images/icon.png"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Islamic",
      "properties": {
        "islamic.interval": {
          "type": "number",
          "default": 0.1,
          "enum": [
            0.1,
            10,
            15,
            30,
            60,
            90,
            120,
            180
          ],
          "description": "Select time between ayat notification."
        },
        "islamic.language": {
          "type": "string",
          "description": "Select preferable language",
          "default": "Arabic",
          "enum": [
            "Arabic",
            "English"
          ]
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.4"
  }
}
